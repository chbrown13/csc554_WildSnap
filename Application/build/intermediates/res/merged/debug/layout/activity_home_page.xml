<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="wrap_content"
    android:layout_height="wrap_content" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="com.samsung.android.sdk.camera.sample.cases.HomePage">


    <EditText
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/editText5"
        android:text="Home Page"
        android:textSize="30dp"
        android:typeface="normal"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:textIsSelectable="false"
        android:layout_centerHorizontal="true"
        android:textAlignment="center"
        android:textStyle="bold|italic"
        android:autoText="false"
        android:editable="false"
        android:textColor="#809b33" />


    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/notifications4"
        android:src="@mipmap/notify_icon"
        android:layout_alignParentBottom="true"
        android:layout_toStartOf="@+id/profile_page4" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/profile_page4"
        android:src="@mipmap/ic_launcher"
        android:layout_gravity="right"
        android:layout_alignTop="@+id/notifications4"
        android:layout_alignParentEnd="true" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/camera4"
        android:src="@mipmap/camera_icon"
        android:layout_gravity="center_horizontal"
        android:layout_alignTop="@+id/notifications4"
        android:layout_toStartOf="@+id/notifications4" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/search4"
        android:src="@mipmap/search_icon"
        android:layout_alignParentBottom="true"
        android:layout_toStartOf="@+id/camera4" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/home4"
        android:src="@mipmap/home_icon"
        android:layout_alignTop="@+id/search4"
        android:layout_toStartOf="@+id/search4" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/editText6"
        android:text="Friends: "
        android:layout_below="@+id/editText5"
        android:layout_alignParentStart="true" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/editText7"
        android:text="New Sightings:"
        android:layout_below="@+id/imageButton"
        android:layout_alignParentStart="true" />

    <ImageView
        android:layout_width="100dp"
        android:layout_height="75dp"
        android:id="@+id/imageView7"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/editText7"
        android:src="@drawable/bird"
        android:scaleType="fitXY" />

    <ImageButton
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:id="@+id/imageButton"
        android:src="@drawable/user_icon2"
        android:scaleType="fitCenter"
        android:layout_below="@+id/editText6"
        android:layout_alignEnd="@+id/editText6" />

    <ImageButton
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:id="@+id/imageButton2"
        android:src="@drawable/user_icon"
        android:scaleType="fitCenter"
        android:layout_above="@+id/editText7"
        android:layout_centerHorizontal="true" />

    <ImageButton
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:id="@+id/imageButton3"
        android:src="@drawable/user_icon3"
        android:scaleType="fitCenter"
        android:layout_alignTop="@+id/imageButton2"
        android:layout_alignEnd="@+id/editText5" />

    <ImageView
        android:layout_width="100dp"
        android:layout_height="75dp"
        android:id="@+id/imageView8"
        android:src="@drawable/fox"
        android:scaleType="fitXY"
        android:layout_below="@+id/editText7"
        android:layout_toEndOf="@+id/editText7" />


</RelativeLayout><!-- From: file:/C:/Users/Christa/Downloads/CameraSDK_v1.0.0/Samples/Camera/Application/src/main/res/layout/activity_home_page.xml -->